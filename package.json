{
  "name": "quick-start",
  "private": true,
  "scripts": {
    "build": "turbo build",
    "dev:web": "turbo run dev --filter=web",
    "dev:server": "turbo run dev --filter=server",
    "dev": "turbo run dev",
    "lint": "turbo lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "rm -rf ./node_modules && rm -rf ./packages/ui/node_modules && rm -rf ./apps/node_modules && rm -rf ./apps/server/node_modules && rm -rf ./apps/web/node_modules"
  },
  "devDependencies": {
    "eslint": "^9.9.1",
    "prettier": "^3.2.5",
    "typescript": "^5.4.5"
  },
  "packageManager": "pnpm@8.15.6",
  "engines": {
    "node": ">=18"
  }
}
